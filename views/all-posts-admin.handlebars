<div class="posts">
  {{#each posts}}
  <article class="card">
    <a class="card-header flex-row justify-space-between" href="/dashboard/edit/{{id}}"></a>
      <h2>
        <a href="/post/{{id}}">{{title}}</a>
      </h2>
      <div>
        Posted on {{format_date createdAt}}
      </div>
    </div>
    <div class="card-body">
      {{body}}
    </div>
  </article>
  {{/each}}
</div>

{{!-- HOME PAGE ONLY --}}


